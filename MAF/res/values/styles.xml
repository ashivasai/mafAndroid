<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    
     <style name="MyCustomTheme" parent="@android:style/Theme.Holo.Light.DarkActionBar">
        <item name="android:actionBarStyle">@style/MyActionBarTheme</item>
         <item name="android:actionBarTabStyle">@style/FindActionBarTheme</item>
        
        
    
    </style>
    
     <style name="MyActionBarTheme" parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse">
        <item name="android:background">#002E4F</item>
        <item name="android:title">@drawable/actionbar_space_between_icon_and_title</item>
        <item name="android:logo">@android:color/transparent</item>
       
    </style>
	<style name="FindActionBarTheme">
    <item name="android:background">@drawable/tab_bar_background</item>
    
    
</style>
    
     <style name="MyTheme" parent="android:Theme.Holo.Light">
    
</style>


	<!-- <style name="FindActionBarTheme">
    
